{
  "name": "npmo-docker",
  "description": "A docker setup for npmo",
  "main": "index.js",
  "scripts": {
    "build": "docker-compose --file config/docker-compose.yml build --no-cache",
    "up": "npm run build && docker-compose --file config/docker-compose.yml up -d && docker ps",
    "replication": "babel-node -- tools/couchdb/replication.js config/replication.config.js"
  },
  "author": "Tom Ashworth <ta@twitter.com>",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.0.15",
    "babel-cli": "^6.1.2",
    "babel-plugin-transform-runtime": "^6.1.2",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-stage-1": "^6.1.2",
    "node-fetch": "^1.3.3"
  }
}
